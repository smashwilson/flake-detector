{
  "name": "flake-detector",
  "version": "1.0.0",
  "description": "Identify flaky tests from GitHub Checks results",
  "author": "Ash Wilson <smashwilson@github.com> (https://github.com/smashwilson/flake-detector)",
  "license": "ISC",
  "repository": "https://github.com//flake-detector.git",
  "homepage": "https://github.com//flake-detector",
  "bugs": "https://github.com//flake-detector/issues",
  "keywords": [
    "probot",
    "github",
    "probot-app"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "dev": "nodemon --exec \"npm start\"",
    "start": "probot run ./lib/index.js",
    "lint": "standard **/*.ts --fix",
    "test": "jest && standard **/*.ts",
    "test:watch": "jest --watch --notify --notifyMode=change --coverage"
  },
  "dependencies": {
    "@types/nock": "^9.3.0",
    "nock": "^10.0.0",
    "probot": "^7.2.0",
    "probot-commands": "1.1.0",
    "probot-config": "0.2.0",
    "probot-metadata": "1.0.0"
  },
  "devDependencies": {
    "@types/dedent": "0.7.0",
    "@types/jest": "^23.1.5",
    "@types/node": "^10.5.2",
    "dedent": "0.7.0",
    "eslint-plugin-typescript": "^0.12.0",
    "jest": "^23.4.0",
    "nodemon": "^1.17.2",
    "smee-client": "^1.0.2",
    "standard": "^10.0.3",
    "ts-jest": "^23.0.0",
    "typescript": "^2.9.2",
    "typescript-eslint-parser": "^18.0.0"
  },
  "engines": {
    "node": ">= 8.3.0"
  },
  "standard": {
    "parser": "typescript-eslint-parser",
    "env": [
      "jest"
    ],
    "plugins": [
      "typescript"
    ]
  },
  "jest": {
    "testEnvironment": "node"
  }
}
